{
  "name": "vue3-starter",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "start": "npm run build && vite preview --port 5050",
    "test": "vitest --run --environment jsdom",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:ci": "start-server-and-test start http-get://localhost:5050/ cy:run",
    "lint": "eslint . --fix --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
    "stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\"",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@vueuse/core": "9.13.0",
    "@vueuse/integrations": "9.13.0",
    "axios": "1.3.4",
    "pinia": "2.0.32",
    "pinia-plugin-persistedstate": "3.1.0",
    "vue": "3.2.47",
    "vue-i18n": "9.2.2",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@testing-library/vue": "6.6.1",
    "@vitejs/plugin-vue": "4.0.0",
    "autoprefixer": "10.4.13",
    "cypress": "12.7.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-vue": "9.9.0",
    "jsdom": "21.1.0",
    "postcss-functions": "4.0.2",
    "postcss-preset-env": "8.0.1",
    "prettier": "2.8.4",
    "sass": "1.58.3",
    "start-server-and-test": "2.0.0",
    "stylelint": "15.2.0",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard-scss": "7.0.1",
    "vite": "4.1.4",
    "vitest": "0.29.2"
  }
}
